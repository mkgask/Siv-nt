{
  "private": true,
  "name": "Siv-nt",
  "description": "Simple image viewer - nextron",
  "version": "0.8.2",
  "author": "mkgask",
  "homepage": "https://zsw.jp",
  "main": "app/background.js",
  "scripts": {
    "dev": "nextron",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "npx jest",
    "test:e2e": "npx playwright test",
    "deploy": "npm run update:package && npm run test:unit && npm run update:license && npm run build",
    "postinstall": "electron-builder install-app-deps",
    "update:package": "npm install && npm update && npm audit fix",
    "update:license": "npx license-checker --summary > licenses.summary.txt && npx license-checker --production --json --excludePrivatePackages --customPath license.customFormat.json --failOn GPL;GPL-2.0;GPL-3.0;AGPL;NGPL > licenses.json",
    "build": "nextron build",
    "build:mac": "nextron build --mac",
    "build:mac:universal": "nextron build --mac --universal",
    "build:linux": "nextron build --linux",
    "build:win32": "nextron build --win --ia32",
    "build:win64": "nextron build --win --x64"
  },
  "devDependencies": {
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.1",
    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.12",
    "@mui/material": "^5.14.10",
    "@playwright/test": "^1.39.0",
    "@types/jest": "^29.5.8",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "canvg": "^4.0.1",
    "copy-webpack-plugin": "^11.0.0",
    "electron": "^26.2.2",
    "electron-builder": "^24.6.4",
    "electron-log": "^4.4.8",
    "electron-serve": "^1.1.0",
    "electron-store": "^8.1.0",
    "image-size": "^1.0.2",
    "jest": "^29.7.0",
    "js-yaml": "^4.1.0",
    "license-checker": "^25.0.1",
    "mime-lite": "^1.0.3",
    "next": "^13.5.4",
    "nextron": "^8.12.0",
    "playwright": "^1.39.0",
    "playwright-electron": "^0.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^9.0.0",
    "rehype-format": "^5.0.0",
    "rehype-raw": "^7.0.0",
    "rehype-sanitize": "^6.0.0",
    "remark-gfm": "^4.0.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "unified": "^11.0.4"
  }
}
